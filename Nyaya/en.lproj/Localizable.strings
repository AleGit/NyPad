// tokenizer

"NYAYA_TOKEN_PATTERN" = "⊨|⊤|⊥|¬|!|∧|&|\\.|∨|\\+|\\||=|↔|<>|→|>|⊻|⊕|\\^|\\(|\\)|,|;|\\w+";
//                          T  F ! ! & & &   +   +  +  = = = > > ^  ^  ^  (   )  , ;  WORDs

"NYAYA_CONNECTIVES_ASCII"    = "F,T,!,&,|,^,>,=";      // comma separated list of ascii symbols for connectives (unused)
"NYAYA_CONNECTIVES_STANDARD" = "⊥,⊤,¬,∧,∨,⊻,→,↔";      // comma separated list of standard symbols for connectives (unused)
"NYAYA_CONNECTIVES_BOOLEAN"  = "0,1,!,.,+,⊕,>,=";      // comma separated list of boolean symbols for connectives (unused)

// parser
// token key = comma separated list of (additional) localized names
// there are implicit declared tokens too
// you can only use symbols which matches NYAYA_TOKENIZER_PATTERN

"F"  = "FALSE,BOTTOM";
"T"  = "TRUE,TOP";
"!"  = "NOT";
"&"  = "AND";
"|"  = "OR";
"="  = "XNOR,BIC,EQ,IFF";
">"  = "IMPL,IMP,IMPLIES";
"^"  = "XOR,EOR";

// "~&" = "NAND";
// "~|" = "NOR";

